              </columns>
            </TableView>
         </children>
      </AnchorPane>
   </center>
   <bottom>
      <AnchorPane prefHeight="103.0" prefWidth="690.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="btnImprimir" layoutX="65.0" layoutY="39.0" mnemonicParsing="false" onAction="#imprimir" text="Imprimir" />
            <Label fx:id="lbImprimir" layoutX="184.0" layoutY="21.0" prefHeight="52.0" prefWidth="440.0" />
         </children>
      </AnchorPane>
   </bottom>
</BorderPane>
