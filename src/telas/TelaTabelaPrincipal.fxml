<BorderPane prefHeight="400.0" prefWidth="690.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controles.TelaTabelaPrincipalController">
   <center>
      <AnchorPane id="AnchorPane" fx:id="scTabela" prefHeight="400.0" prefWidth="674.0">
